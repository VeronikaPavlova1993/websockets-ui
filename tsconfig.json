{
 "compilerOptions": {
  "outDir": "./dist/",
  "target": "esnext",
  "module": "esnext",
  "esModuleInterop": true,
  "forceConsistentCasingInFileNames": true,
  "strict": true,
  "skipLibCheck": true,
  "moduleResolution": "Node",
  "sourceMap": true,
  "allowJs": true,
  "noImplicitAny": true
 },
 "include": ["src/**/*"],
 "exclude": ["front/", "node_modules"]
}
